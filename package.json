{
  "name": "petshopapi",
  "version": "1.0.0",
  "description": "API para dados de Petshop",
  "scripts": {
    "start": "npm run-script start_babel",
    "build": "babel . -d dist --presets es2015,stage-2 --ignore spec.js,node_modules",
    "serve": "node dist/server.js",
    "start_babel": "babel-node ./server.js --preset=babel-preset-es6",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luiznds62/qual-a-sala-api.git"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "Express"
  ],
  "author": "Luiz Henrique Naspolini",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.15.2",
    "cors": "^2.8.0",
    "express": "4.14.0",
    "lodash": "^4.15.0",
    "mongoose": "^5.7.7",
    "morgan": "^1.8.2",
    "pg": "^7.15.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-inline-import": "^2.0.1",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "nodemon": "^1.19.4"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
